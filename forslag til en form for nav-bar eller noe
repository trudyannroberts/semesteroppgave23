<span style="font-size:30px;color: #666666;cursor:pointer" onclick="openNav()">&#9776; meny</span>

<script>
<!-- "script" = javascript-->
function openNav() {
  document.getElementById("mySidenav").style.width = "250px";
<!-- åpner baren og bestemmer hvor mye av baren som skal vises når man trykker på den -->
}

function closeNav() {
  document.getElementById("mySidenav").style.width = "0px";
<!-- lukker baren og bestemmer hvor mye skal synes av baren når den lukkes -->
}
</script>



